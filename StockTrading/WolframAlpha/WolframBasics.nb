<body class="layout vertical">

<main id="main-page-wrapper">
    <div id="page-content">
  <div id="sign-in-username" class="container">
    <div id="wolf-oauth" class="center-block">
      <form id="user-form" role="form" novalidate="true" action="oauth2/sign-in" method="post"><div id="form-logo">
          <div class="svg-image" id="wolfram-logo"></div>
        </div>
        <div id="form-container">
          <div id="form-main">
            <h3 class="red">Sign In</h3>
